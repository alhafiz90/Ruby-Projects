for (c = 0 ; c < ( n - 1 ); c++)
  {
    for (d = 0 ; d < n - c - 1; d++)
    {
      if (array[d] > array[d+1]) /* For decreasing order use < */
      {
        swap       = array[d];
        array[d]   = array[d+1];
        array[d+1] = swap;
      }
    }
  }
  


file=File.new('csv2.csv','w')
for e in a do
  str_array=e.split(';')
  for x in str_array do
    file.write x + "\t\t\t"
  end
  file.puts